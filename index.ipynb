    "# Calculate sum of squared errors between regression and mean line \n",
    "import numpy as np\n",
    "\n",
    "def sq_err(y_a, y_b):\n",
    "    return sum([(y_a[i] - y_b[i])**2 for i in range(len(y_a))])\n",
